{
  "name": "butterchurn",
  "version": "3.0.0-beta.3",
  "description": "Butterchurn is a WebGL implementation of the Milkdrop Visualizer",
  "main": "dist/butterchurn.js",
  "unpkg": "dist/butterchurn.js",
  "files": [
    "dist/butterchurn.js",
    "dist/butterchurn.min.js",
    "dist/isSupported.min.js"
  ],
  "scripts": {
    "start": "http-server -S -C ../../cert.pem -K ../../key.pem",
    "build": "webpack --config config/webpack.config.js --env prod",
    "dev": "webpack --config config/webpack.config.js --progress --color --watch --env dev",
    "dev-build": "webpack --config config/webpack.config.js --progress --color --env dev",
    "build:preset": "webpack  --config preset-docs/webpack.config.js --mode production"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jberg/butterchurn.git"
  },
  "keywords": [
    "music visualization",
    "visualizer",
    "webgl",
    "webaudio"
  ],
  "author": "Jordan Berg <jordannealberg@gmail.com>",
  "license": "MIT",
  "homepage": "https://butterchurnviz.com",
  "dependencies": {
    "@assemblyscript/loader": "^0.27.9",
    "@babel/runtime": "^7.22.11",
    "ecma-proposal-math-extensions": "0.0.2",
    "eel-wasm": "^0.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "assemblyscript": "^0.27.9",
    "babel-loader": "^9.1.3",
    "eslint": "^8.48.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "prettier": "^3.0.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "yargs": "^17.7.2"
  },
  "browserslist": "supports audio-api and supports webgl2 and last 3 years"
}
